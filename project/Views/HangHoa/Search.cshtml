
@using static project.Helpers.PaginationHelper
@model PagedResult<project.ViewModels.HangHoaVM>

@{
    ViewData["Title"] = "Search";
    Layout = "DSHangHoa";
    var totalPages = ViewData["TotalPages"] != null ? (int)ViewData["TotalPages"] : 0;
    var currentPage = ViewData["CurrentPage"] != null ? (int)ViewData["CurrentPage"] : 1;
    var currentQuery = ViewData["CurrentQuery"] as string;

    
}


@await Html.PartialAsync("ProductItem", Model.Items)


@if (Model != null)
{
    <partial name="Pagination" model="Model" />
}

